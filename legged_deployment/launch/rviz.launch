<launch>
  <arg name="robot_type" default="aliengo"/>
  <arg name="rvizconfig" default="$(find legged_deployment)/config/rviz.rviz" />

  <param name="$(arg robot_type)" textfile="/tmp/legged_control/$(arg robot_type).urdf" />
  <param name="legged_robot_description" command="$(find xacro)/xacro $(find custom_description)/urdf/robot.xacro
        robot_type:=$(arg robot_type) sim:=true"/>
  
  <node pkg="rviz" type="rviz" name="rviz" args="-d $(arg rvizconfig)" output="screen" />

</launch>